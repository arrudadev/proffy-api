{
  "compilerOptions": {    
    "target": "es2017",
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": ".",
    
    "noImplicitThis": false,
    
    "strict": true,
    "strictPropertyInitialization": false,
    
    "esModuleInterop": true,
    
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}